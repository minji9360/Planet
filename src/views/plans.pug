extends layouts/main.pug
include mixins/planBlock.pug

block content
    section
        a.prev__btn < 
        .day
            - const order = [ 0, 1, 2, 3, 4, 5, 6 ]
            - const days = [ "일", "월", "화", "수", "목", "금", "토"]
            ol.day__list#slider
                each i in order
                    li.day__item
                        - let todaySentence
                        .title
                            .title__date
                                span=thisWeek.date[i]
                                |일 
                                span.day=days[thisWeek.day[i]]
                                |요일
                            .title__description
                                - let flag = 0
                                - if (user.sentence==null)
                                    p.description 입력창 왼쪽 버튼을 눌러 입력해주세요.
                                - else
                                    each item in user.sentence
                                        - if (item.daily===true && thisWeek.year[i]==item.year && thisWeek.month[i]==item.month && thisWeek.date[i]==item.date)
                                            p.description(id=`description${i}`) !{item.content.replace(/\n/g, '<br />')}
                                            - flag = 1
                                            - todaySentence = JSON.stringify(item)
                                            - break
                                    - if (flag==0)
                                        p.description 입력창 왼쪽 버튼을 눌러 입력해주세요.
                        .todolist-box
                            ul.todolist
                                - if (user.plans!=null)
                                    each item in user.plans
                                        - if (thisWeek.year[i]==item.year && thisWeek.month[i]==item.month && thisWeek.date[i]==item.date)
                                            +planBlock(
                                                i,
                                                {
                                                id:item._id,
                                                title:item.title,
                                                content:item.content,
                                                important:item.important,
                                                completed:item.completed,
                                                feedback:item.feedback,
                                            })
                        form.add-form(id=`addForm${i}`, method="post", autocomplete="off")
                            .title(id=`titleBox${i}`)
                                a.option__button(type="button", onclick=`slidePlanDetail(${i});`)
                                    img.up__image(id=`upImage${i}`, src="data:image/png;base64,")
                                    img.down__image.hidden(id=`downImage${i}`, src="data:image/png;base64,")
                                input.title__input(id=`titleInput${i}`, type="text", name="title", placeholder="할 일", required)
                                .title-plan
                                    - if (i > 0)
                                        button.add-title__button(id=`addButton${i}`, formaction=`plans${routes.uploadPlan}`) 추가
                                    - else
                                        button.add-title__button(id=`addButton${i}`, formaction=`plans${routes.uploadPlan}`, disabled) 추가
                                    button.edit-check__button.hidden(id=`editButton${i}`, formaction=`plans${routes.editPlan}`) 수정
                                .title-feedback
                                    button.add-title__button.hidden(id=`addCommentButton${i}`, formaction=`plans${routes.uploadFeedback}`) 추가
                                    button.edit-check__button.hidden(id=`editCommentButton${i}`, formaction=``) 수정
                            .detail(id=`detailBox${i}`)
                                textarea.detail__textarea(id=`contentInput${i}`, name="content", placeholder="상세 내용을 입력해주세요.")
                                .sentence__button
                                    - if (i > 0)
                                        button.add-detail__button.hidden(id=`contentButton${i}`, formaction=`plans${routes.uploadSentence}`) 추가
                                    - else
                                        button.add-detail__button.hidden(id=`contentButton${i}`, formaction=`plans${routes.uploadSentence}`, disabled) 추가
                                    button.edit-check__button.hidden(id=`editSentenceButton${i}`, formaction=`plans${routes.editSentence}`) 수정
                                button.feedback-delete__button.hidden(id=`deleteCommentButton${i}`, formaction=``) 삭제
                                .detail-buttons
                                    label(for=`important${i}`, id=`importantButton${i}`, onclick=`updateImportant(${i});`)
                                        img.gray__button(id=`importantGray${i}`, src="data:image/png;base64,")
                                        img.color__button.hidden(id=`importantColor${i}`, src="data:image/png;base64,")
                                        | 중요
                                    .sentence-buttons.hidden(id=`sentenceButtons${i}`)
                                        label(for=`senteceDaily${i}`, onclick=`clickDaily(${i}, ${todaySentence});`)
                                            img.gray__button.hidden(id=`dailyGray${i}`, src="data:image/png;base64,")
                                            img.color__button(id=`dailyColor${i}`, src="data:image/png;base64,")
                                            | 오늘의 목표&nbsp;
                                        label(for=`senteceGoal${i}`, onclick=`clickGoal(${i});`)
                                            img.gray__button(id=`goalGray${i}`, src="data:image/png;base64,")
                                            img.color__button.hidden(id=`goalColor${i}`, src="data:image/png;base64,")
                                            | 최상위 목표
                                    a.sentence__button(id=`sentenceButton${i}`, type="button", onclick=`clickSentence(${i}, ${todaySentence});`)
                                        img.gray__button(id=`sentenceGray${i}`, src="data:image/png;base64,")
                                        img.color__button.hidden(id=`sentenceColor${i}`, src="data:image/png;base64,")
                                        | 목표
                                    .rating.hidden(id=`rating${i}`)
                                        label(for=`rating${i}1`, onclick=`checkRating(${i}, 1);`)
                                            img.gray-star(id=`starGray${i}1`, src="data:image/png;base64,")
                                            img.color-star.hidden(id=`starColor${i}1`, src="data:image/png;base64,")
                                        label(for=`rating${i}2`, onclick=`checkRating(${i}, 2);`)
                                            img.gray-star(id=`starGray${i}2`, src="data:image/png;base64,")
                                            img.color-star.hidden(id=`starColor${i}2`, src="data:image/png;base64,")
                                        label(for=`rating${i}3`, onclick=`checkRating(${i}, 3);`)
                                            img.gray-star(id=`starGray${i}3`, src="data:image/png;base64,")
                                            img.color-star.hidden(id=`starColor${i}3`, src="data:image/png;base64,")
                                        label(for=`rating${i}4`, onclick=`checkRating(${i}, 4);`)
                                            img.gray-star(id=`starGray${i}4`, src="data:image/png;base64,")
                                            img.color-star.hidden(id=`starColor${i}4`, src="data:image/png;base64,")
                                        label(for=`rating${i}5`, onclick=`checkRating(${i}, 5);`)
                                            img.gray-star(id=`starGray${i}5`, src="data:image/png;base64,")
                                            img.color-star.hidden(id=`starColor${i}5`, src="data:image/png;base64,")
                                    input.hidden(id=`important${i}`, type="checkbox", name="important", value="false")
                                    input.hidden(id=`senteceDaily${i}`, type="radio", name="daily", value="true", checked)
                                    input.hidden(id=`senteceGoal${i}`, type="radio", name="daily", value="false")
                                    each score in [1, 2, 3, 4, 5]
                                        input.hidden(id=`rating${i}${score}`, type="radio", name="rating", value=score)
                                    input(id=`idInput${i}`, type="hidden", name="id")
                                    input(type="hidden", name="category", value="공부")
                                    input(type="hidden", name="year", value=thisWeek.year[i])
                                    input(type="hidden", name="month", value=thisWeek.month[i])
                                    input(type="hidden", name="date", value=thisWeek.date[i])
        a.next__btn > 
